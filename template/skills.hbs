{{#if skills}}
<section id="skills">
    <header class="title">Habilidades</header>
    <div class="content">
      {{#each skills}}
      <ul class="skill-list">
        {{#each this}}
        <li>
          <span>{{this.title}} - {{this.level}}</span>
        </li>
        {{/each}}
      </ul>
      {{/each}}
    </div>
</section>
{{/if}}
